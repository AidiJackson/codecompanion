  "reasons": [
    {
      "package": "streamlit + data science stack (numpy, pandas, scipy, etc.)",
      "reason": "Only used in dashboard component - moved to optional dependency",
      "impact": "Reduces install size by ~300MB, eliminates 40+ transitive dependencies"
    },
    {
      "package": "google-generativeai, openai",
      "reason": "Multiple AI clients installed but typically only one needed",
      "impact": "Reduces API client bloat, kept anthropic as primary client"
    },
    {
      "package": "requests, aiohttp", 
      "reason": "Multiple HTTP clients providing same functionality as httpx",
      "impact": "Eliminates redundant HTTP implementations"
    },
    {
      "package": "redis, gevent, tornado",
      "reason": "Heavy server/async dependencies not used in core functionality", 
      "impact": "Reduces complexity and potential security surface"
    },
    {
      "package": "Development tools in main requirements",
      "reason": "pytest, ruff moved to requirements-dev.txt for cleaner production builds",
      "impact": "Separates dev dependencies from runtime requirements"
    },
    {
      "package": "Transitive dependency bloat",
      "reason": "Many packages pulled in unused transitive dependencies",
      "impact": "Eliminated ~50+ packages that provide no value to core functionality"
    },
    {
      "package": "pydantic-settings",
      "reason": "Added for proper configuration management used in settings.py",
      "impact": "Explicitly declares configuration dependency instead of relying on transitive"
    }
  ]
}